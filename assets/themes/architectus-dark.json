  "tokenColors": [
    {
      "name": "Comment - Blueprint annotations",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#4a6b8f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword - Structural directives",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#8ab4f8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String - Geometric definitions",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#8ad4f8"
      }
    },
    {
      "name": "Number - Coordinate values",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#5dbaff"
      }
    },
    {
      "name": "Function - Construction methods",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#e8f4ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class - Platonic forms",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#c8e0ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable - Render parameters",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#6fa0e8"
      }
    },
    {
      "name": "Constant - Immutable geometry",
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#5dbaff"
      }
    },
    {
      "name": "Operator - Spatial operations",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#5a8dd8"
      }
    },
    {
      "name": "Tag - Structural markers",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#8ab4f8"
      }
    },
    {
      "name": "Attribute - Geometric properties",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#8ad4f8"
      }
    },
    {
      "name": "Type - Architectural types",
      "scope": ["support.type", "entity.name.type"],
      "settings": {
        "foreground": "#8a9bf8"
      }
    },
    {
      "name": "Import - Structural dependencies",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#8ab4f8",
        "fontStyle": "italic"
      }
    }
  ]
}

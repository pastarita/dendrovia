    "titleBar.inactiveBackground": "#2a1f16",
    "titleBar.inactiveForeground": "#6b5d4f",
    "titleBar.border": "#4a382280",
    "menu.background": "#362a1d",
    "menu.foreground": "#a6896b",
    "menu.selectionBackground": "#4a382260",
    "menu.selectionForeground": "#f5ead6",
    "menu.border": "#4a382280",
    "menubar.selectionBackground": "#4a382260",
    "menubar.selectionForeground": "#f5ead6",
    "editor.lineHighlightBackground": "#4a382220",
    "editor.lineHighlightBorder": "#4a382200",

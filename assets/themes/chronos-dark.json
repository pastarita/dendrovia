    "terminal.ansiBrightGreen": "#74a5d4",
    "terminal.ansiBrightYellow": "#f5ead6",
    "terminal.ansiBrightBlue": "#74a5d4",
    "terminal.ansiBrightMagenta": "#d9c3a5",
    "terminal.ansiBrightCyan": "#74a5d4",
    "terminal.ansiBrightWhite": "#f5ead6",
    "gitDecoration.modifiedResourceForeground": "#dda15e",
    "gitDecoration.deletedResourceForeground": "#b85c38",
    "gitDecoration.untrackedResourceForeground": "#74a5d4",
    "gitDecoration.ignoredResourceForeground": "#6b5d4f",
    "gitDecoration.conflictingResourceForeground": "#b85c38",
    "badge.background": "#dda15e",
    "badge.foreground": "#2a1f16",
    "button.background": "#8b7355",
    "button.foreground": "#f5ead6",
    "button.hoverBackground": "#a6896b",
    "input.background": "#4a382240",
    "input.border": "#4a382280",
    "input.foreground": "#e8d7c3",
    "input.placeholderForeground": "#6b5d4f",
    "inputOption.activeBorder": "#d4a574",
    "inputValidation.errorBackground": "#b85c38",
    "inputValidation.errorBorder": "#b85c38",
    "scrollbarSlider.background": "#4a382260",
    "scrollbarSlider.hoverBackground": "#4a382280",
    "scrollbarSlider.activeBackground": "#4a3822a0",
    "tab.activeBackground": "#2a1f16",
    "tab.activeForeground": "#e8d7c3",
    "tab.activeBorder": "#d4a574",
    "tab.inactiveBackground": "#362a1d",
    "tab.inactiveForeground": "#6b5d4f",
    "tab.border": "#4a382280",
    "breadcrumb.foreground": "#a6896b",
    "breadcrumb.background": "#2a1f16",
    "breadcrumb.focusForeground": "#e8d7c3",
    "breadcrumb.activeSelectionForeground": "#d4a574",
    "breadcrumbPicker.background": "#362a1d"
  },
  "tokenColors": [
    {
      "name": "Comment - Archaeological notation",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6b5d4f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword - Temporal markers",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#d4a574",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String - Ancient text",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#c49060"
      }
    },
    {
      "name": "Number - Temporal coordinates",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#dda15e"
      }
    },
    {
      "name": "Function - Excavation tools",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#f5ead6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class - Artifact classification",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#e8d7c3",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable - Fossil data",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#a6896b"
      }
    },
    {
      "name": "Constant - Preserved values",
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#dda15e"
      }
    },
    {
      "name": "Operator - Stratigraphic relations",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#8b7355"
      }
    },
    {
      "name": "Tag - Temporal markers",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#d4a574"
      }
    },
    {
      "name": "Attribute - Artifact properties",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#c49060"
      }
    },
    {
      "name": "Type - Sedimentary classification",
      "scope": ["support.type", "entity.name.type"],
      "settings": {
        "foreground": "#74a5d4"
      }
    },
    {
      "name": "Import - Archaeological cross-reference",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#d4a574",
        "fontStyle": "italic"
      }
    }
  ]
}

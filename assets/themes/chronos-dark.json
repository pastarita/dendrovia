    "editorWhitespace.foreground": "#4a382260",
    "editorIndentGuide.background": "#4a382230",
    "editorIndentGuide.activeBackground": "#4a382260",
    "editorLineNumber.foreground": "#6b5d4f",
    "editorLineNumber.activeForeground": "#a6896b",
    "editorBracketMatch.background": "#8b735540",
    "editorBracketMatch.border": "#8b735580",
    "editorGutter.background": "#2a1f16",
    "editorGutter.addedBackground": "#74a5d460",
    "editorGutter.modifiedBackground": "#dda15e60",
    "editorGutter.deletedBackground": "#b85c3860",
    "diffEditor.insertedTextBackground": "#74a5d420",
    "diffEditor.removedTextBackground": "#b85c3820",
    "editorWidget.background": "#362a1d",
    "editorWidget.border": "#4a382280",
    "editorSuggestWidget.background": "#362a1d",
    "editorSuggestWidget.border": "#4a382280",
    "editorSuggestWidget.foreground": "#a6896b",
    "editorSuggestWidget.selectedBackground": "#4a382260",
    "editorSuggestWidget.highlightForeground": "#dda15e",
    "editorHoverWidget.background": "#362a1d",
    "editorHoverWidget.border": "#4a382280",
    "peekView.border": "#d4a574",
    "peekViewEditor.background": "#2a1f16",
    "peekViewResult.background": "#362a1d",
    "peekViewTitle.background": "#4a3822",
    "panel.background": "#2a1f16",
    "panel.border": "#4a382280",
    "panelTitle.activeBorder": "#d4a574",
    "panelTitle.activeForeground": "#e8d7c3",
    "panelTitle.inactiveForeground": "#6b5d4f",
    "terminal.background": "#2a1f16",
    "terminal.foreground": "#e8d7c3",
    "terminal.ansiBlack": "#4a3822",
    "terminal.ansiRed": "#b85c38",
    "terminal.ansiGreen": "#74a5d4",
    "terminal.ansiYellow": "#dda15e",

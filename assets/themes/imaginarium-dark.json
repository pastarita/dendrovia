  "tokenColors": [
    {
      "name": "Comment - Alchemical notation",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#7a5fa0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword - Transformation operators",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c6a0f6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String - Ethereal essence",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#d0a0f6"
      }
    },
    {
      "name": "Number - Distillation parameters",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#da9ef7"
      }
    },
    {
      "name": "Function - Compilation vessels",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#f8f0ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class - Alchemical constructs",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#e5d4ff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable - Procedural essence",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#b088e0"
      }
    },
    {
      "name": "Constant - Crystallized values",
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#da9ef7"
      }
    },
    {
      "name": "Operator - Transmutation symbols",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#9b6dd8"
      }
    },
    {
      "name": "Tag - Spectrum markers",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#c6a0f6"
      }
    },
    {
      "name": "Attribute - Color properties",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#d0a0f6"
      }
    },
    {
      "name": "Type - Mathematical essence",
      "scope": ["support.type", "entity.name.type"],
      "settings": {
        "foreground": "#a0c6f6"
      }
    },
    {
      "name": "Import - Alchemical ingredients",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#c6a0f6",
        "fontStyle": "italic"
      }
    }
  ]
}

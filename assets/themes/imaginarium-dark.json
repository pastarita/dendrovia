    "terminal.ansiBrightBlue": "#a0c6f6",
    "terminal.ansiBrightMagenta": "#d0a0f6",
    "terminal.ansiBrightCyan": "#a0c6f6",
    "terminal.ansiBrightWhite": "#f8f0ff",

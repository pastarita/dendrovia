    "editorBracketMatch.background": "#9b6dd840",
    "editorBracketMatch.border": "#9b6dd880",
    "editorGutter.background": "#1a0f1f",
    "editorGutter.addedBackground": "#a0c6f660",
    "editorGutter.modifiedBackground": "#da9ef760",

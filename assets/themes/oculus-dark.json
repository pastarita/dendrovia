    "list.inactiveSelectionBackground": "#5f3d2d40",
    "list.hoverBackground": "#5f3d2d30",
    "list.focusBackground": "#5f3d2d60",
    "list.highlightForeground": "#ffb366",
    "statusBar.background": "#5f3d2d",
    "statusBar.foreground": "#e89b6f",

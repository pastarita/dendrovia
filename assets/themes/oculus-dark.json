    "input.placeholderForeground": "#9d7357",
    "inputOption.activeBorder": "#f5a97f",
    "inputValidation.errorBackground": "#ff6b66",
    "inputValidation.errorBorder": "#ff6b66",
    "scrollbarSlider.background": "#5f3d2d60",
    "scrollbarSlider.hoverBackground": "#5f3d2d80",
    "scrollbarSlider.activeBackground": "#5f3d2da0",
    "tab.activeBackground": "#1f1410",
    "tab.activeForeground": "#ffd4b8",
    "tab.activeBorder": "#f5a97f",
    "tab.inactiveBackground": "#2a1a12",
    "tab.inactiveForeground": "#9d7357",
    "tab.border": "#5f3d2d80",
    "breadcrumb.foreground": "#e89b6f",
    "breadcrumb.background": "#1f1410",
    "breadcrumb.focusForeground": "#ffd4b8",
    "breadcrumb.activeSelectionForeground": "#f5a97f",
    "breadcrumbPicker.background": "#2a1a12"
  },
  "tokenColors": [
    {
      "name": "Comment - Contextual hints",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#9d7357",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keyword - Interface directives",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#f5a97f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String - Display text",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#f5c27f"
      }
    },
    {
      "name": "Number - UI coordinates",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffb366"
      }
    },
    {
      "name": "Function - View methods",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#fff5ed",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Class - Component types",
      "scope": ["entity.name.class", "entity.name.type"],
      "settings": {
        "foreground": "#ffd4b8",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable - UI state",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#e89b6f"
      }
    },
    {
      "name": "Constant - Fixed values",
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#ffb366"
      }
    },
    {
      "name": "Operator - View operations",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#d88957"
      }
    },
    {
      "name": "Tag - UI elements",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#f5a97f"
      }
    },
    {
      "name": "Attribute - Element properties",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#f5c27f"
      }
    },
    {
      "name": "Type - Component types",
      "scope": ["support.type", "entity.name.type"],
      "settings": {
        "foreground": "#7fa9f5"
      }
    },
    {
      "name": "Import - Component imports",
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#f5a97f",
        "fontStyle": "italic"
      }
    }
  ]
}

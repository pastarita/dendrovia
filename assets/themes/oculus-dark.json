    "terminal.ansiBrightRed": "#ff6b66",
    "terminal.ansiBrightGreen": "#f5c27f",
    "terminal.ansiBrightYellow": "#fff5ed",
    "terminal.ansiBrightBlue": "#7fa9f5",
    "terminal.ansiBrightMagenta": "#f57f9b",
    "terminal.ansiBrightCyan": "#7fa9f5",
    "terminal.ansiBrightWhite": "#fff5ed",
    "gitDecoration.modifiedResourceForeground": "#ffb366",
    "gitDecoration.deletedResourceForeground": "#ff6b66",
    "gitDecoration.untrackedResourceForeground": "#7fa9f5",
    "gitDecoration.ignoredResourceForeground": "#9d7357",
    "gitDecoration.conflictingResourceForeground": "#ff6b66",
    "badge.background": "#ffb366",
    "badge.foreground": "#1f1410",

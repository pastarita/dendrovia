    "titleBar.inactiveBackground": "#1f1410",
    "titleBar.inactiveForeground": "#9d7357",
    "titleBar.border": "#5f3d2d80",
    "menu.background": "#2a1a12",
    "menu.foreground": "#e89b6f",
    "menu.selectionBackground": "#5f3d2d60",
    "menu.selectionForeground": "#fff5ed",
    "menu.border": "#5f3d2d80",
    "menubar.selectionBackground": "#5f3d2d60",
    "menubar.selectionForeground": "#fff5ed",
    "editor.lineHighlightBackground": "#5f3d2d20",
    "editor.lineHighlightBorder": "#5f3d2d00",

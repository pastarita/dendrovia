    "editorWhitespace.foreground": "#5f3d2d60",
    "editorIndentGuide.background": "#5f3d2d30",
    "editorIndentGuide.activeBackground": "#5f3d2d60",
    "editorLineNumber.foreground": "#9d7357",
    "editorLineNumber.activeForeground": "#e89b6f",
    "editorBracketMatch.background": "#d8895740",
    "editorBracketMatch.border": "#d8895780",
    "editorGutter.background": "#1f1410",
    "editorGutter.addedBackground": "#7fa9f560",
    "editorGutter.modifiedBackground": "#ffb36660",
    "editorGutter.deletedBackground": "#ff6b6660",
    "diffEditor.insertedTextBackground": "#7fa9f520",
    "diffEditor.removedTextBackground": "#ff6b6620",
    "editorWidget.background": "#2a1a12",
    "editorWidget.border": "#5f3d2d80",
    "editorSuggestWidget.background": "#2a1a12",
    "editorSuggestWidget.border": "#5f3d2d80",
    "editorSuggestWidget.foreground": "#e89b6f",
    "editorSuggestWidget.selectedBackground": "#5f3d2d60",
    "editorSuggestWidget.highlightForeground": "#ffb366",
    "editorHoverWidget.background": "#2a1a12",
    "editorHoverWidget.border": "#5f3d2d80",
    "peekView.border": "#f5a97f",
    "peekViewEditor.background": "#1f1410",
    "peekViewResult.background": "#2a1a12",
    "peekViewTitle.background": "#5f3d2d",
    "panel.background": "#1f1410",
    "panel.border": "#5f3d2d80",
    "panelTitle.activeBorder": "#f5a97f",
    "panelTitle.activeForeground": "#ffd4b8",
    "panelTitle.inactiveForeground": "#9d7357",
    "terminal.background": "#1f1410",
    "terminal.foreground": "#ffd4b8",
    "terminal.ansiBlack": "#5f3d2d",
    "terminal.ansiRed": "#ff6b66",
    "terminal.ansiGreen": "#f5c27f",
    "terminal.ansiYellow": "#ffb366",

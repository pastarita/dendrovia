{
  "name": "@dendrovia/imaginarium",
  "version": "0.1.0",
  "type": "module",
  "description": "The Compiler - Distills AI art into mathematical shaders",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./distillation": "./src/distillation/index.ts",
    "./generation": "./src/generation/index.ts",
    "./cache": "./src/cache/DeterministicCache.ts",
    "./fallbacks": "./src/fallback/index.ts",
    "./utils/color": "./src/utils/color.ts",
    "./utils/glsl": "./src/utils/glsl.ts",
    "./mycology": "./src/mycology/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts --watch",
    "distill": "bun run src/distill.ts",
    "build": "tsc --noEmit",
    "test": "bun test",
    "clean": "rm -rf generated/palettes/* generated/shaders/* generated/noise/* generated/lsystems/* generated/manifest.json"
  },
  "dependencies": {
    "@dendrovia/shared": "workspace:*",
    "react": "^19.2.0"
  },
  "devDependencies": {
    "typescript": "^5.7.2",
    "@types/node": "^22.10.5"
  }
}
